{
  "name": "@cupid-ui/react",
  "version": "0.1.4",
  "description": "A modern framework for building responsive frontends in React.",
  "main": "dist/index.js",
  "module": "dist/cupid-ui.esm.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/designbylunar/cupid-ui.git",
    "directory": "packages/react"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsdx build --name cupid-ui --format cjs,esm,umd --tsconfig ./tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "playground": "preact build --src playground/ --out playgroundBuild/ && http-server playgroundBuild/ -b -c-1"
  },
  "author": "The Lunar Design Team <npm@lunardesign.org>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "preact": {
      "optional": true
    }
  },
  "peerDependencies": {
    "preact": "^10.0.0",
    "react": "^17.0.0"
  },
  "dependencies": {
    "@cupid-ui/css": "0.0.3",
    "http-server": "^0.12.3"
  },
  "devDependencies": {
    "@types/node": "15.6.0",
    "@types/react": "17.0.8",
    "@types/react-dom": "17.0.5",
    "preact": "10.5.13",
    "preact-cli": "3.2.0",
    "preact-render-to-string": "5.1.19",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}
