{
  "name": "ui-framework",
  "version": "0.1.0",
  "description": "A modern framework for building responive frontends",
  "main": "index.js",
  "repository": "https://github.com/designbylunar/ui-framework.git",
  "author": "The Lunar Design Team <npm@lunardesign.org>",
  "license": "MIT",
  "scripts": {
    "build": "postcss src/ui.css -o dist/ui.css && postcss src/tailwind.css -o dist/tailwind.css",
    "watch": "postcss src/ui.css -o dist/ui.css --watch",
    "dev": "yarn build && yarn watch"
  },
  "homepage": "https://lunardesign.org/projects/ui-framework",
  "private": false,
  "devDependencies": {
    "autoprefixer": "10.1.0",
    "cssnano": "4.1.10",
    "postcss": "8.2.2",
    "postcss-cli": "8.3.1",
    "postcss-each": "0.10.0",
    "postcss-import": "14.0.0",
    "postcss-nested": "5.0.3",
    "postcss-preset-env": "6.7.0",
    "tailwindcss": "2.0.2"
  }
}
