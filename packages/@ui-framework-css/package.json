{
  "name": "@ui-framework/css",
  "version": "0.1.0",
  "description": "A modern framework for buliding responsive frontends.",
  "main": "dist/ui-framework.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/designbylunar/ui-framework.git",
    "directory": "packages/@ui-framework-css"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "postcss src/ui-framework.css -o dist/ui-framework.css",
    "watch": "postcss src/ui-framework.css -o dist/ui-framework.css --watch",
    "dev": "npm run build; npm run watch"
  },
  "author": "The Lunar Design Team <npm@lunardesign.org>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "10.1.0",
    "cssnano": "4.1.10",
    "postcss": "8.2.10",
    "postcss-cli": "8.3.1",
    "postcss-each": "1.0.0",
    "postcss-import": "14.0.2",
    "postcss-nested": "5.0.5",
    "postcss-preset-env": "6.7.0"
  }
}
